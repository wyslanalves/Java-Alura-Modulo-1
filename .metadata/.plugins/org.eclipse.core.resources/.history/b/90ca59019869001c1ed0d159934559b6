
public class Cliente implements Autenticavel {

	
	private AutenticacaoUtill util;
	
	
	public Cliente() {
		this.util = new AutenticacaoUtill();	
	}
	
	
	@Override
	public void setSenha(int senha) {
		this.setSenha(senha);
	}

	@Override
	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}

	}

}
