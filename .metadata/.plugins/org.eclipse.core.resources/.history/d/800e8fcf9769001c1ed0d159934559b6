
public class Cliente implements Autenticavel {

	
	private Autenticacao util;
	
	
	public Cliente() {
	this. = new AutenticacaoUtill();	
	}
	
	
	@Override
	public void setSenha(int senha) {
		this.senha = senha;
	}

	@Override
	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}

	}

}
