
public class TesteReferencias {

	public static void main(String[] args) {
		
		Gerente g = new Gerente();
		g1.setNome("wyslan");
		g1.setSalario(5000.0);
		
		
		Funcionario f = new Funcionario();
		f.setSalario(2000.0);
		
		ControleBonificacao controle = new ControleBonificacao();
